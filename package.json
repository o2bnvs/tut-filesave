{
  "name": "tut-filesave",
  "version": "1.0.0",
  "description": "periodic backups of the lireddit tutorial",
  "main": "index.js",
  "scripts": {
    "watch": "tsc -w",
    "start": "node dist/index.js",
    "start2": "ts-node src/index.ts",
    "dev": "nodemon dist/index.js",
    "dev2": "nodemon --exec ts-node src/index.ts"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/o2bnvs/tut-filesave.git"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/o2bnvs/tut-filesave/issues"
  },
  "homepage": "https://github.com/o2bnvs/tut-filesave#readme",
  "devDependencies": {
    "@types/node": "^15.12.2",
    "nodemon": "^2.0.7",
    "ts-node": "^10.0.0",
    "typescript": "^4.3.2"
  },
  "dependencies": {
    "@mikro-orm/cli": "^4.5.6",
    "@mikro-orm/core": "^4.5.6",
    "@mikro-orm/migrations": "^4.5.6",
    "@mikro-orm/postgresql": "^4.5.6",
    "pg": "^8.6.0"
  },
  "mikro-orm": {
    "useTsNode": true,
    "configPaths": [
      "./src/mikro-orm.config.ts",
      "./dist/mikro-orm.config.js"
    ]
  }
}
